    "swagger": "2.0",
    "info": {
        "description": "Some description",
        "title": "Go Family Tree API",
        "termsOfService": "http://swagger.io/terms/",
        "contact": {
            "name": "API Support",
            "url": "http://www.swagger.io/support",
